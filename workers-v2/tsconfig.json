{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../dist/workers-v2",
    "rootDir": "..",
    "module": "commonjs",
    "target": "es2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "**/*.ts",
    "../lib/db.ts",
    "../lib/providers.ts",
    "../lib/chains.ts",
    "../lib/dex.ts"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "../lib/createDexPool.ts"
  ]
}
